@model IEnumerable<Projet1.Models.Domiciliatione>

@{
    ViewData["Title"] = "Index";
    Layout = "_ALayout";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.idUtilisateur)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idDocument)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idAdresseDomiciliation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateDebut)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateFin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.etat)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.idUtilisateur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idDocument)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idAdresseDomiciliation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateDebut)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateFin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.etat)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
